{
    "name": "webstasolutions/laratex",
    "description": "A package for creating PDFs in Laravel using LaTeX",
    "keywords": [
        "laratex"
    ],
    "homepage": "https://github.com/webstasolutions/laratex",
    "license": "MIT",
    "authors": [
        {
            "name": "Ismael Wismann",
            "email": "wismann.ismael@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Jan Bl√°ha",
            "email": "jan.blaha@websta.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "5.4.x|5.5.X|5.6.X|5.7.X|5.8.X|^6.0|^7.0|^8.0",
        "illuminate/filesystem": "5.4.x|5.5.X|5.6.X|5.7.X|5.8.X|^6.0|^7.0|^8.0",
        "symfony/process": "^2.0|^3.0|^4.0|^5.0",
        "ext-zip": "*"
    },
    "autoload": {
        "psr-4": {
            "Websta\\LaraTeX\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Websta\\LaraTeX\\LaraTeXServiceProvider"
            ],
            "aliases": {
                "LaraTeX": "Websta\\LaraTeX\\LaraTeXFacade"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan vendor:publish --provider='Websta\\LaraTeX\\LaraTeXServiceProvider' --tag=public --force"
        ]
    }
}
